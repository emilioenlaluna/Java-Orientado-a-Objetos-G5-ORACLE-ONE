package app010;

public class Caracter {
	public static void main(String[] args) {
		//Caractetes
		char caracter = 'a';
		System.out.print("Letra"+caracter);
		caracter=48;
		System.out.print("ASCII"+caracter);
		
		//Conversion de numero a char
		
		char letra=48;
		letra=(char)(65+1);
		System.out.print("Letra"+caracter);
		
	}
}
